<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="estudiantes_odoo_sequence" model="ir.sequence">
            <field name="name">Secuencia Matricula para Estudiantes Odoo</field>
            <field name="code">estudiantes.odoo</field>
            <field name="prefix">MAT%(year)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="materia_precision_decimal_id" model="decimal.precision">
            <field name="name">Costo Materia</field>
            <field name="digits">4</field>
        </record>

    </data>

    <data>
        <record id="pasatiempo_01" model="pasatiempo.odoo">
            <field name="name">Futbol</field>
        </record>
        <record id="pasatiempo_02" model="pasatiempo.odoo">
            <field name="name">Correr</field>
        </record>
        <record id="pasatiempo_03" model="pasatiempo.odoo">
            <field name="name">Facebook</field>
        </record>

    </data>

</odoo>